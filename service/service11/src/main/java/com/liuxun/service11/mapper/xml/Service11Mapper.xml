<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liuxun.service11.mapper.Service11Dao">
    <update id="updateStoreByid" parameterType="com.liuxun.model.service11.ProductVo">
        update product set store=${store} where product_id = #{productId}
    </update>
    <select id="findAll">
        select * from product
    </select>
</mapper>